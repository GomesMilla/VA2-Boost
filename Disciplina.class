package main;

class Disciplina {
    String nome;
    Professor professor;
    float notaFinal;

    // Criando os professores com o construstor
    Disciplina(String nome, String nome, float notaFinal){
        Professor professor = new Professor(nome);
        nome = nome;
        this.notaFinal = notaFinal;
    }

    public void ExibirRelatorioDisc(){
        System.out.println("A disciplina" + nome + "lecionada pelo professor(a)" + professor.nome + "obteve nota final de" + notaFinal);
    }

    public float RetornarNotaFinal(){
        return notaFinal;
    }
  
  }
  
